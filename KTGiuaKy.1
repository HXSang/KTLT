#include <stdio.h>
#include <conio.h>
long X(int n)
{
    if(n == 1 || n == 2)
    {
        return 1;
    }
    else if(n % 2 == 0 && n > 2)
    {
        return 2 * X(n-1) + 1;
    }
    else if(n % 2 == 1 && n > 2)
    {
        return X(n-1) + X(n-2);
    }
}
int main()
{
    int n;
    printf("\nNhap so n : ");
    scanf("%d",&n);
    printf("\nPhan tu thu %d cua day X la : ",n);
    printf("%ld",X(n));
}
